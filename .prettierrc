{
  "printWidth": 100,
  "singleQuote": false,
  "jsxSingleQuote": true,
  "trailingComma": "none",
  "semi": true,
  "tabWidth": 2,
  "useTabs": false,
  "endOfLine": "lf",
  "arrowParens": "always",
  "importOrder": ["^@images/(.*)$", "^@components/(.*)$", "^@fonts/(.*)$", "^[./]"],
  "importOrderSeparation": true,
  "importOrderGroupNamespaceSpecifiers": true,
  "importOrderSortSpecifiers": true,
  "tailwindFunctions": ["cn", "cva"],
  "plugins": ["@trivago/prettier-plugin-sort-imports"]
}
